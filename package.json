{
  "name": "parolla",
  "version": "2.1.6",
  "description": "Kelime oyunu",
  "private": true,
  "keywords": [
    "oyun",
    "kelime oyunu",
    "bulmaca",
    "pasaparola"
  ],
  "author": "selimdoyranli",
  "contributors": [
    "Selim DoyranlÄ± <selim@radkod.com> (https://selimdoyranli.com)"
  ],
  "bugs": "selim@radkod.com",
  "homepage": "https://github.com/RadKod/parolla",
  "repository": {
    "type": "git",
    "url": "https://github.com/RadKod/parolla"
  },
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development && nuxt --port 3000",
    "prod": "cross-env NODE_ENV=production && nuxt start --port 3000",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint:eslint": "eslint --ext .js,.vue,.pug --ignore-path .gitignore --ignore-path .eslintignore .",
    "lint:eslint:fix": "eslint --fix --ext .js,.vue,.pug --ignore-path .gitignore --ignore-path .eslintignore .",
    "lint:stylelint": "stylelint --ignore-path .stylelintignore \"{assets/style,components,layouts,pages}/**/*.{css,sass,scss,less,stylus,vue}\"",
    "lint:stylelint:fix": "stylelint --ignore-path .stylelintignore \"{assets/style,components,layouts,pages}/**/*.{css,sass,scss,less,stylus,vue}\" --fix",
    "prettier": "prettier --config ./.prettierrc.js --ignore-path ./.prettierignore --write \"**/*.{js,json,css,scss,vue,html,pug}\"",
    "changelog": "changelogen"
  },
  "lint-staged": {
    "*.{js,vue,json}": [
      "npm run lint:eslint",
      "npm run prettier"
    ],
    "{assets/style,components,layouts,pages}/**/*.{css,sass,scss,less,stylus,vue}": [
      "npm run lint:stylelint",
      "npm run prettier"
    ]
  },
  "dependencies": {
    "@nuxtjs/composition-api": "^0.33.1",
    "@nuxtjs/gtm": "^2.4.0",
    "bootstrap": "5.2.3",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "howler": "^2.2.3",
    "nuxt": "2.16.3",
    "nuxt-user-agent": "^1.2.2",
    "simple-keyboard": "^3.4.65",
    "swiper": "^8.0.6",
    "textfit": "^2.4.0",
    "vant": "^2.12.44",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@nuxtjs/color-mode": "2.1.0",
    "@nuxtjs/eslint-module": "^3.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@prettier/plugin-pug": "^1.10.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "changelogen": "^0.5.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "nuxt-font-loader": "^1.1.3",
    "postcss": "^8.4.21",
    "postcss-preset-env": "^7.5.0",
    "prettier": "^2.1.2",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass": "^1.60.0",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
